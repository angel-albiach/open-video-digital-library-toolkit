.account-page
  .head
    %span.left
      Administration Dashboard
  .body
    /left col start
    .left.col-right
      %dl.lt#library-tab
        %dt
          Library Management
        %dd
          %ul
            - mgmt_tabs.each do |tab|
              = tabs_tab_for tab
        %dt
          Metadata Management
        %dd
          %ul
            - md_tabs.each do |tab|
              = tabs_tab_for tab
    /left col end, main col start
    .col-main.right{ :style => "width: 620px" }
      / - form_for :library, @library, :html => { :method => :put, :class => "admin-form create" } do |@form|
      - tabs.each do |tab|
        = tabs_div3_for tab
