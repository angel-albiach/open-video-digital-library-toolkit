- form_for :library, @library, :html => { :method => :put } do |@f|
  %fieldset.library
    %legend Collection Information
    .tabs
      %ul
        - tabs.each do |tab|
          = tab_for tab
      - tabs.each do |tab|
        = div_for tab
    .clear

    = @f.submit "Save"
    = link_to "cancel", library_path
